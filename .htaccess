Options -MultiViews
<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /imo/renda-passiva-calc/
  RewriteRule ^index\.html$ - [L]
  
  # Se o ficheiro ou diretório não existir, redirecionar para index.html
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteCond %{REQUEST_FILENAME} !-l
  RewriteRule . /imo/renda-passiva-calc/index.html [L]
</IfModule>
